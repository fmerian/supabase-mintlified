{
  "$schema": "https://mintlify.com/schema.json",

  "name": "supabase docs",

  "logo": {
    "dark": "/logo/supabase-logo-wordmark--dark.svg",
    "light": "/logo/supabase-logo-wordmark--light.svg"
  },

  "favicon": "/supabase-logo-icon.svg",

  "colors": {
    "primary": "#019A55",
    "light": "#3ECF8E",
    "dark": "#019A55",
    "background": {
      "light": "#FCFCFC",
      "dark": "#121212"
    }
  },

  "versions": ["vecs", "auth"],

  "topbarCtaButton": {
    "name": "Sign up",
    "style": "roundedRectangle",
    "url": "https://supabase.com/dashboard",
    "arrow": false
  },

  "navigation": [
    {
      "group": "Getting Started",
      "version": "vecs",
      "pages": ["introduction", "vecs/api", "vecs/hosting"]
    },
    {
      "group": "Concepts",
      "version": "vecs",
      "pages": [
        "vecs/concepts/adapters",
        "vecs/concepts/collections",
        "vecs/concepts/indexes",
        "vecs/concepts/metadata"
      ]
    },
    {
      "group": "Integrations",
      "version": "vecs",
      "pages": [
        "vecs/integrations/amazon_bedrock",
        "vecs/integrations/huggingface_inference_endpoints",
        "vecs/integrations/openai"
      ]
    },
    {
      "group": "Support",
      "version": "vecs",
      "pages": ["vecs/support/changelog"]
    },
    {
      "group": "Documentation",
      "version": "auth",
      "pages": [
        "auth/introduction",
        "auth/quickstart",
        "auth/production",
        "auth/configuration"
      ]
    }
  ],

  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },

  "footerSocials": {
    "github": "https://github.com/supabase",
    "x": "https://x.com/supabase",
    "discord": "https://discord.supabase.com/"
  }
}
